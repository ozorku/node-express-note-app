<div class="notepad">
  <h2>
    Notes
  </h2>
  <ul>
    {{#each data}}
      <li>
        {{this.note}}
            <button class="deleteNote" value={{this.id}}>DELETE</button>
      </li>
    {{/each}}
  </ul>
  <form class="new-note" method="POST" action="addNote">
    <input
      name="newNote"
      placeholder="Write note here..."
      type="text"
      value=""
    />
    <button>
      Add note
    </button>
  </form>
</div>